{{>header2}}

<h1>
    Appointments
</h1>
<table class="table table-striped">
     <thead>
    <tr>
         <th>email</th>
        <th>Name</th>
        <th>mobile</th>
        <th>Age</th>
        <th>Date</th>
        <th>Time</th>
        <th>Doctor</th>
        <th>Specified</th>
       
    </tr>
    </thead>
    <tbody >
   {{#each data}}
    <tr>
        <td>{{this.email}}</td>
        <td>{{this.name}}</td>
         <td>{{this.mobile}}</td>
        <td>{{this.age}}</td>
        <td>{{this.date}}</td>
        <td>{{this.time}}</td>
        <td>{{this.doctor}}</td>
        <td>{{this.specified}}</td>
    </tr>
{{/each}}
    </tbody>
</table>
</div>