

<h1>
    Appointments
</h1>
<table class="table table-striped">
     <thead>
    <tr>
        
        <th>date</th>
        <th>time</th>
        <th>doctor</th>
        <th>specified</th>
    </tr>
    </thead>
    <tbody >
    {{#each data}}
    <tr>
        <td>{{this.date}}</td>
        <td>{{this.time}}</td>
        <td>{{this.doctor}}</td>
        <td>{{this.specified}}</td>
    </tr>
    {{/each}}
    </tbody>
</table>
</div>